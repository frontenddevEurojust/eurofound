
div.horizontal-tabs {
    border: 1px solid #CCCCCC;
    margin: 20px 0 -1px 0;
    padding: 0;
    position: relative;
	border:0px;	
    overflow: hidden;
}
.horizontal-tabs ul.horizontal-tabs-list {
    border: 0 ;
    float: left;
    height: auto;
    list-style: none outside none;
    margin: 0 0 -1px 0;
    padding: 0;
    position: relative;
    width: 100%;
    background: none;
}
.horizontal-tabs fieldset.horizontal-tabs-pane {
    border: 0 none;
    padding: 0 0;
    clear: both;
}
.horizontal-tabs-pane > legend {
    display: none;
}
.horizontal-tabs ul.horizontal-tabs-list li {
    background: none !important;
    float: left;
    border-color: transparent;
    border-style: solid;
    border-width: 1px 1px 0 1px;
    padding: 0 0 0 0;
}
.horizontal-tabs ul.horizontal-tabs-list li.selected {
    background-color: #f5f5f5 !important;
    padding: 0 0 0 0;
    border-color: #A5CFBD;
}



.horizontal-tabs ul.horizontal-tabs-list li a,
.horizontal-tabs ul.horizontal-tabs-list li.selected a {
    display: block;
    text-decoration: none;
    padding: 0.5em 1.2em 0.5em 1.2em;
    position: relative;
    top: 0;
}
.contextTab a,
.outcomesTab a,
.timeSeriesTab a,
.timeLineTab a,
.countryInfoTab a{
	padding: 1em 1.2em 1em 3.6em !important;
	background-image: url('../img/tabs-icons.png');
    background-repeat: no-repeat;
}
.horizontal-tabs ul.horizontal-tabs-list li a:hover{
	background-color: transparent;
}
.contextTab a{
	background-position: 10px 9px;
}
.contextTab.selected a{
	background-position: 10px -42px;
}
.outcomesTab a{
	background-position: 10px -97px;
}
.outcomesTab.selected a{
	background-position: 10px -147px;
}
.timeSeriesTab a{
	background-position: 10px -197px;
}
.timeSeriesTab.selected a{
	background-position: 10px -243px;
}
.timeLineTab a{
	background-position: 10px -295px;
}
.timeLineTab.selected a{
	background-position: 10px -348px;
}
.countryInfoTab a{
	background-position: 10px -407px;
}
.countryInfoTab.selected a{
	background-position: 10px -461px;
}


.horizontal-tabs ul.horizontal-tabs-list li.selected strong{
	font-weight: 700;
	text-shadow: 1px 1px 0px #fff;
}










.horizontal-tabs ul.horizontal-tabs-list strong {
    color: none;
    font-weight: 300;
}
.horizontal-tabs ul.horizontal-tabs-list .summary {
    display: block;
}
.horizontal-tabs ul.horizontal-tabs ul.horizontal-tabs-list .summary {
    line-height: normal;
    margin-bottom: 0;
}
div.field-group-htabs-wrapper .field-group-format-wrapper {
    clear: both;
    padding: 0 0 0.6em;
}
.horizontal-tabs .horizontal-tab-hidden {
    display: block;
    position: absolute;
    top: -100000px;
}


