msgid "Save configuration"
msgstr "Guardar configuración"
msgid "enabled"
msgstr "activado"
msgid "disabled"
msgstr "desactivado"
msgid "Disabled"
msgstr "Desactivado"
msgid "Enabled"
msgstr "Activado"
