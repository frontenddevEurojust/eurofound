msgstr "Kord päevas"
msgid "Weekly"
msgstr "Kord nädalas"
msgid "Update"
msgstr "Uuenda"
msgid "Modules"
msgstr "Moodulid"
msgid "Available updates"
msgstr "Saadaolevad uuendused"
msgid "Up to date"
msgstr "Värske"
msgid "Update available"
msgstr "Uuendus saadaval"
msgid "Languages"
msgstr "Keeled"
msgid "warning"
msgstr "hoiatus"
msgid "Themes"
msgstr "Teemad"
msgid "Multilingual"
msgstr "Mitmekeelne"
