msgstr ""

msgid "Previous Image"
msgstr ""

msgid "Left Arrow"
msgstr ""

msgid "Next Image"
msgstr ""

msgid "Right Arrow"
msgstr ""

msgid "Toggle Zoom"
msgstr ""

msgid "z (not available in slideshow)"
msgstr ""

msgid "Toggle Play / Pause"
msgstr ""

