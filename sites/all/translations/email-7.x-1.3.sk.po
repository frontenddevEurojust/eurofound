msgstr "Predmet nemôže obsahovať konce riadkov."
msgid "Email Contact Form"
msgstr "Kontaktný formulár pre email"
msgid "\"%mail\" is not a valid email address"
msgstr "\"%mail\" nie je platná emailová adresa"
